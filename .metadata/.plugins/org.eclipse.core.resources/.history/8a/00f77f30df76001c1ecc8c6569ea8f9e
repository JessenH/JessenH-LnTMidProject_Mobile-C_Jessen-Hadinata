
public class Main {

	public Main() {
		int menu =0;
		
		do {
			
			System.out.println(" Movie Rental");
			System.out.println("===============");
			System.out.println("1. Add new movie");
			System.out.println("2. View movie");
			System.out.println("3. Rent movie");
			System.out.println("4. Exit");
			System.out.print(">> ");
			
		} while (menu != 4);
		
		
		
		
		

		
		
	}

	public static void main(String[] args) {
		new Main();

	}

}
